{
  "extends": ["eslint:recommended", "plugin:meteor/recommended","plugin:vue/recommended","airbnb"],
  "settings": {
      "import/resolver": "meteor"
  },
  "parser": "vue-eslint-parser",
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "mocha": true
  },
  "rules": {
    "no-param-reassign": [2, { "props": false }],
    "no-console": "off",
    "no-multi-spaces": "off"
  }
}
